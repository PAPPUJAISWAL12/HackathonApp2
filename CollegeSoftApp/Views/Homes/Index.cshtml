         <form method="POST" asp-action="Index" asp-controller="Homes">
            <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Login</h5>
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="text-danger"><p>@ViewBag.Error</p></div>
                            <div class="form-group mt-4">
                                <label asp-for="UserEmail">User Name:</label>
                                <input asp-for="UserEmail" type="text" required="" placeholder="Enter Your UserName" class="form-control" />
                            </div>
                            <div class="form-group">
                                <label asp-for="Upassword">Password:</label>
                                <input type="password" asp-for="Upassword" id="psw-icon" placeholder="*****" class="form-control" />
                                <i class="fa-solid fa-eye psw-icon" id="status" onclick="ViewPassword()"></i>
                                <!-- <i class="fa-solid fa-eye-slash"></i> -->
                                <script>
                                    var x = document.getElementById("psw-icon");
                                    var y = document.getElementById("status");
                                    function ViewPassword() {
                                        if (x.type == 'password') {
                                            x.type = 'text';
                                            y.className = "fa-solid fa-eye-slash psw-icon";
                                        } else {
                                            x.type = 'password';
                                            y.className = "fa-solid fa-eye psw-icon";
                                        }
                                    }
                                </script>
                            </div>
                        </div>
                        <div class="modal-footer">
                             <input type="submit" value="Login" class="btn btn-login">
                        </div>
                    </div>
                </div>
            </div>           
        </form>
